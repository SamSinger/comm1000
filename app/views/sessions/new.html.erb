<%= render 'shared/content_title', title: 'Log in' %>

<div class='well'>
  <%= form_tag '/login' do %>
    <div class='control-group'>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
    <div class='control-group'>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <%= submit_tag 'Login', class: 'btn btn-success' %>
  <% end %>

   Not yet registered?  
    <%= link_to 'Register', register_path  %>
</div>